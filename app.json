{
	"expo": {
		"name": "SubscriptionPlus",
		"slug": "SubscriptionPlus",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/sub-icon-base.png",
		"scheme": "subscriptionplus",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#E6F4FE",
				"foregroundImage": "./assets/images/sub-icon-base.png"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"permissions": ["NOTIFICATIONS", "WAKE_LOCK"],
			"useNextNotificationsApi": true,
			"package": "com.noneandundefined.SubscriptionPlus",
			"versionCode": 1
		},
		"web": {
			"output": "static",
			"favicon": "./assets/images/sub-icon-base.png",
			"bundler": "metro"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/sub-icon-base.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff",
					"dark": {
						"backgroundColor": "#000000"
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "9013a758-c6e2-481c-a2c6-c9a525275d24"
			}
		},
		"build": {
			"preview": {
				"distribution": "internal",
				"android": {
					"buildType": "apk"
				}
			}
		}
	}
}
